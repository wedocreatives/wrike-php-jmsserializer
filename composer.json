{
    "name": "wedocreatives/wrike-php-jmsserializer",
    "description": "Response transformer adapter for Wrike PHP Library, based on JMS Serializer package.",
    "keywords": ["wrike", "wrike.com", "api", "sdk", "library", "jmsserializer", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zbigniew Ślązak"
        }
    ],
    "require": {
        "php": ">=5.5",
        "wedocreatives/wrike-php-library": "dev-master",
        "jms/serializer": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0,<6.0"
    },
    "autoload": {
        "psr-4": {"wedocreatives\\WrikePhpJmsserializer\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "wedocreatives\\WrikePhpJmsserializer\\Tests\\": "tests/" }
    }
}
